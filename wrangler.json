{
  "name": "whiteboardrealtime",
  "main": "src/server/index.ts",
  "compatibility_date": "2023-10-01",
  "workers_dev": true,
  "durable_objects": {
    "bindings": [
      {
        "name": "WhieteboardRealTime",
        "class_name": "WhieteboardRealTime"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_classes": ["WhieteboardRealTime"]
    }
  ],
  "assets": {
    "directory": "./public",
    "binding": "ASSETS",
    "not_found": "dist/index.html"
  },
  "build": {
    "command": "esbuild src/client/index.ts --bundle --splitting --format=esm --platform=browser --outdir=public/dist"
  },
  "websocket": true,
  "upload_format": "service-worker",
  "compatibility_flags": ["streams_enable_constructors", "nodejs_compat"]
}